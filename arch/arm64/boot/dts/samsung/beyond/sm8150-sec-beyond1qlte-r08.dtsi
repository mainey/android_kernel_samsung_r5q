#include "sm8150-sec-beyond1qlte-common.dtsi"
#include "../../../../../drivers/motor/tuning/beyond1qlte_motor_tuning.dtsi"
#include "sm8150-sec-beyond1qlte-optics-r08.dtsi"

&soc {
	rpmh-regulator-ldoa17 {
		L17A: pm8150_l17: regulator-pm8150-l17 {
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <3000000>;
			qcom,init-voltage = <3000000>;
		};
	};

	i2c_17: i2c@17 { /* SW I2C */
		status = "ok";

		cell-index = <17>;
		compatible = "i2c-gpio";
		gpios = <&tlmm 79 0 /* sda */
			 &tlmm 80 0 /* scl */
		>;
		#i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		a96t3x6@20 {
			compatible = "a96t3x6";
			reg = <0x20>;

			interrupt-parent = <&tlmm>;
			interrupts = <125 0>;

			a96t3x6,irq_gpio = <&tlmm 125 0>;
			a96t3x6,dvdd_vreg_name = "pm8150_l17";
			a96t3x6,fw_path = "abov/a96t356_beyond1qlte.bin";
			a96t3x6,firmup_cmd = <0x3b>;
			a96t3x6,earjack_noise = <1>;
		};
	};
};
&usb_qmp_dp_phy {
	vdd-supply = <&pm8150_l18>;
	qcom,vdd-voltage-level = <0 912000 912000>;
};

&i2c_23 {
	status = "disabled";
};

&spmi_bus {
	qcom,pm8150@0 {
		qcom,power-on@800 {
			qcom,pon_2 {
				linux,code = <KEY_WINK>;
			};
		};
	};
};

&gpio_keys {
	vol_up {
		linux,code = <KEY_VOLUMEDOWN>;
	};

	key_wink {
		linux,code = <KEY_VOLUMEUP>;
	};
};
