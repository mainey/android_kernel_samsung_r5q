#include "battery-data-d1q-common.dtsi"

&qupv3_se4_i2c {
	pca9468@57 {
		pca9468,chg_gpio_en = <&expander_gpios 2 0>; /* DC_CPEN */
	};
};

&i2c_22 {
	mfc-charger@3b {
		battery,mst_pwr_en = <&expander_gpios 21 0>; /* MST_PWR_EN */
	};
};

&soc {
	battery {
		battery,wpc_en = <&expander_gpios 3 0>; /* WPC_EN */
	};
};
