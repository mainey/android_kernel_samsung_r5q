	qupv3_se8_cam_i2c_pins: qupv3_se8_cam_i2c_pins {
		qupv3_se8_cam_i2c_active: qupv3_se8_cam_i2c_active {
			mux {
				pins = "gpio88", "gpio89";
				function = "qup8";
			};

			config {
				pins = "gpio88", "gpio89";
				drive-strength = <2>;
				bias-disable;
			};
		};	
		qupv3_se8_cam_i2c_sleep: qupv3_se8_cam_i2c_sleep {
