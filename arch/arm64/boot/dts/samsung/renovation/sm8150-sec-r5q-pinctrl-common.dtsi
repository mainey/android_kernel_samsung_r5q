                        pins = "gpio14";
                        function = "cam_mclk";
                };
                config {
                        pins = "gpio14";
                        bias-pull-down; /* PULL DOWN */
                        drive-strength = <4>; /* 4 MA */
                };
        };
        cam_sensor_mipi_sw_active: cam_sensor_mipi_sw_active {
		/* MIPI_SW */
		mux {
			pins = "gpio116";
			function = "gpio";
		};
		config {
			pins = "gpio116";
			bias-disable; /* No PULL */
			output-high;
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mipi_sw_suspend: cam_sensor_mipi_sw_suspend {
